                        <TextView
                            android:id="@+id/subreasonLabel"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:fontFamily="@font/ttcommons_regular"
                            android:text="Sub-Reason"
                            android:textColor="@color/dark_gray"
                            android:textSize="@dimen/dimen_14sp" />

                        <TextView
                            android:id="@+id/subreasonTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:fontFamily="@font/ttcommons_demibold"
                            android:gravity="end"
                            android:textColor="@color/very_dark_gray"
                            android:textSize="@dimen/dimen_14sp" />
                    </LinearLayout>

                    <View
                        android:id="@+id/checkinDivider"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="12dp"
                        android:background="@color/very_light_gray"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/checkinHeaderContainer" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </com.google.android.material.card.MaterialCardView>

            <ng.max.vams.customview.ProgressButton
                android:id="@+id/continueButton"
                android:layout_width="0dp"
                android:layout_height="@dimen/dimen_55dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/vehicleDetailCard"
                app:layout_constraintStart_toStartOf="@+id/vehicleDetailCard"
                app:layout_constraintTop_toBottomOf="@+id/guideline15"
